#include <iostream>

using namespace std;

class Base
{
	public:
		virtual void print()
		{
			cout<<"Base class print"<<endl;
		}
		void show()
		{
			cout<<"Base class show"<<endl;
		}
};

class Derived : public Base
{
	public:
		void print()
		{
			cout<<"Derived class print"<<endl;
		}
		void show()
		{
			cout<<"Derived class show"<<endl;
		}
};

int main()
{
	Base *bptr;
	Derived dobj;
	bptr = &dobj;
	
	bptr->print();
	bptr->show();

	exit(0);
}
